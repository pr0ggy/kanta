{
    "name": "pr0ggy/kanta",
    "description": "An easy-to-use, exception-based functional assertion library for PHP 5.6 and later",
    "version": "0.1.1",
    "type": "library",
    "keywords": ["php", "unit", "testing", "assert", "assertion", "validation"],
    "license": "GPL",
    "require": {
        "php": ">=5.6.0",
        "sebastian/comparator": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4" : {
            "Kanta\\Validation\\" : "lib/"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Kanta\\Test\\" : "tests/"
        },
        "files": []
    }
}
